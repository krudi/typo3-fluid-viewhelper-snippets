{
    "name": "typo3-viewhelpers-vscode-snippets",
    "version": "0.1.3",
    "description": "Integrate TYPO3 code snippets easily to Visual Studio Code. Improve your TYPO3 development with a collection of popular snippets, designed for quick access and efficient coding.",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://gitlab.com/krudi/typo3-viewhelpers-vscode-snippet/LICENSE"
        }
    ],
    "publisher": "krudi",
    "private": false,
    "homepage": "https://gitlab.com/krudi/typo3-viewhelpers-vscode-snippets#readme",
