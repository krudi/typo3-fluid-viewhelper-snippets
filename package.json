{
    "name": "typo3-fluid-viewhelper-snippets",
    "version": "0.3.0",
    "description": "Integrate TYPO3 code snippets easily to Visual Studio Code. Improve your TYPO3 development with a collection of popular snippets, designed for quick access and efficient coding.",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://gitlab.com/krudi/typo3-fluid-viewhelper-snippets/LICENSE"
        }
    ],
    "publisher": "krudi",
    "private": false,
    "homepage": "https://gitlab.com/krudi/typo3-fluid-viewhelper-snippets#readme",
    "bugs": {
        "url": "https://gitlab.com/krudi/typo3-fluid-viewhelper-snippets/issue"
    },
    "keywords": [
        "vscode",
        "extension",
        "snippets",
        "typo3",
        "fluid",
        "viewhelpers"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/krudi/typo3-fluid-viewhelper-snippets.git"
    },
    "engines": {
        "vscode": "^1.0.0"
    },
    "scripts": {
        "lint:files": "prettier --check \"**/*.{js,ts,css,html,yaml,yml,json,json5,graphql,gql}\"",
        "lint:files:fix": "prettier --write \"**/*.{js,ts,css,html,yaml,yml,json,json5,graphql,gql}\""
    },
    "preview": true,
    "categories": [
        "Snippets"
    ],
    "icon": "./assets/images/icon.png",
    "displayName": "TYPO3 Fluid ViewHelper Snippets",
    "galleryBanner": {
        "color": "#FFFFFF",
        "theme": "light"
    },
    "contributes": {
        "languages": [
            {
                "id": "tsconfig",
                "aliases": [
                    "TSConfig",
                    "tsconfig"
                ],
                "extensions": [
                    ".tsconfig"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "typoscript",
                "aliases": [
                    "TypoScript",
                    "typoscript"
                ],
                "extensions": [
                    ".typoscript"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tsconfig",
                "scopeName": "source.tsconfig",
                "path": "./syntaxes/tsconfig.tmLanguage.json"
            },
            {
                "language": "typoscript",
                "scopeName": "source.typoscript",
                "path": "./syntaxes/typoscript.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/f/form/f.form.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/form/f.form.select.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/asset/f.asset.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/cache/f.cache.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/debug/f.debug.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/format/f.format.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/link/f.link.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/sanitize/f.sanitize.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/security/f.security.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/transform/f.transform.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/uri/f.uri.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/f.general.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/be/f.be.general.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/be/menu/f.be.menus.json"
            },
            {
                "language": "html",
                "path": "./snippets/f/be/security/f.be.security.json"
            },
            {
                "language": "html",
                "path": "./snippets/be/link/be.link.json"
            },
            {
                "language": "html",
                "path": "./snippets/be/uri/be.uri.json"
            },
            {
                "language": "html",
                "path": "./snippets/be/be.general.json"
            },
            {
                "language": "html",
                "path": "./snippets/core/core.general.json"
            },
            {
                "language": "html",
                "path": "./snippets/formvh/formvh.form.json"
            },
            {
                "language": "html",
                "path": "./snippets/formvh/formvh.general.json"
            },
            {
                "language": "html",
                "path": "./snippets/cb/cb.general.json"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^22.17.2",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3"
    }
}
